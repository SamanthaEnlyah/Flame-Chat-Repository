<Window x:Class="FlameChatClient.GUI.PersonalityTraits"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlameChatClient.GUI"
        mc:Ignorable="d"
        Title="PersonalityTraits" Height="503" Width="226" Loaded="Window_Loaded">
    <Grid Margin="0,0,0,9">
        <Grid.RowDefinitions>
            <RowDefinition Height="7*"/>
            <RowDefinition Height="23*"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="listOfAllTraits" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="19,20,22,72" Grid.RowSpan="2"/>
        <Button x:Name="BtnAddToUserList" Content="Add" HorizontalAlignment="Center" Margin="0,308,0,0" VerticalAlignment="Top" Width="88" Height="37" Grid.Row="1" Click="BtnAddToUserList_Click" RenderTransformOrigin="-4.717,1.421"/>

    </Grid>
</Window>
